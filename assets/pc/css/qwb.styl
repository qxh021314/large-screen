.qwb
  width $w = 790px
  height $h = 300px
  display flex
  justify-content space-around

  .qwb-bd
    display flex
    justify-content space-between
    // h - title - title/bottom
    height ($h - 35px - 10px)

  .qwb-panel
    width ($w / 2) - 5px
    & > table
      width ($w / 2) - 5px
      & > tbody > tr
        height 35px
        text-align center
